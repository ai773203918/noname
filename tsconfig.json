{
    "exclude": [
        "dist",
        "output",
        "node_modules"
    ],
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "strict": true,
        "noImplicitAny": false,
        "allowSyntheticDefaultImports": true,
    }
}